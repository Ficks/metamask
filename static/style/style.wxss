* {
  margin: 0;
  padding: 0;
  border: 0;
  -webkit-font-smoothing: antialiased;
  -moz-osx-font-smoothing: grayscale;
  -webkit-tap-highlight-color: rgba(0, 0, 0, 0);
  font-family: Calibre, Helvetica, Arial, sans-serif;
}
img {
  max-width: 100%;
}
h1,
h2,
h3,
h4,
h5,
h6 {
  color: #2a2825;
  line-height: 1.2;
  font-weight: 500;
  letter-spacing: -0.3px;
  margin: 0px;
}
h1 {
  display: block;
  margin-block-start: 0.67em;
  margin-block-end: 0.67em;
  margin-inline-start: 0px;
  margin-inline-end: 0px;
  font-weight: bold;
  font-weight: normal;
  font-size: 70px;
  color: #2a2825;
  letter-spacing: -3px;
}
li {
  list-style: none;
}
.container {
  margin: 0 auto;
  max-width: 1200px;
}
.img {
  font-size: 0;
}
h2 {
  font-weight: normal;
  font-size: 40px;
}
